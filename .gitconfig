[user]
	name = rtakasuke
	email = takahashi@tamagohan.skr.jp
[alias]
	st = status
	ci = commit
	co = checkout
	up = pull
	br = branch
[core]
	editor = /usr/bin/vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
